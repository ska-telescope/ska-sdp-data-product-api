{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "interface": {
        "type": "string"
      },
      "execution_block": {
        "type": "string"
      },
      "context": {
        "type": "object",
        "properties": {
          "observer": {
            "type": "string"
          },
          "intent": {
            "type": "string"
          },
          "notes": {
            "type": "string"
          }
        },
        "required": [
          "observer",
          "intent",
          "notes"
        ]
      },
      "config": {
        "type": "object",
        "properties": {
          "processing_block": {
            "type": "string"
          },
          "processing_script": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "version": {
            "type": "string"
          },
          "commit": {
            "type": "string"
          },
          "cmdline": {
            "type": "string"
          }
        },
        "required": [
          "processing_block",
          "processing_script",
          "image",
          "version",
          "commit",
          "cmdline"
        ]
      },
      "files": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "path": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "description": {
                "type": "string"
              }
            },
            "required": [
              "path",
              "status",
              "description"
            ]
          }
        ]
      },
      "obscore": {
        "type": "object",
        "properties": {
          "dataproduct_type": {
            "type": "string",
            "maxLength": 50
          },
          "dataproduct_subtype": {
            "type": "string",
            "maxLength": 20
          },
          "calib_level": {
            "type": "integer",
            "minimum": -32768,
            "maximum": 32767
          },
          "obs_collection": {
            "type": "string",
            "maxLength": 255
          },
          "obs_id": {
            "type": "string",
            "maxLength": 8
          },
          "obs_title": {
            "type": "string",
            "maxLength": 255
          },
          "obs_creator_name": {
            "type": "string",
            "maxLength": 255,
            "default": "SKA Observatory"
          },
          "obs_publisher_did": {
            "type": "string",
            "maxLength": 255
          },
          "obs_release_date": {
            "type": "string"
          },
          "access_url": {
            "type": "string",
            "maxLength": 255
          },
          "access_format": {
            "type": "string",
            "maxLength": 50
          },
          "access_estsize": {
            "type": "integer",
            "minimum": -9223372036854776000,
            "maximum": 9223372036854776000
          },
          "target_name": {
            "type": "string",
            "maxLength": 255
          },
          "s_ra": {
            "type": "number"
          },
          "s_dec": {
            "type": "number"
          },
          "s_fov": {
            "type": "number"
          },
          "s_region": {
            "type": "string",
            "maxLength": 255
          },
          "s_resolution": {
            "type": "number"
          },
          "s_xel1": {
            "type": "integer",
            "minimum": -2147483648,
            "maximum": 2147483647
          },
          "s_xel2": {
            "type": "integer",
            "minimum": -2147483648,
            "maximum": 2147483647
          },
          "t_min": {
            "type": "number"
          },
          "t_max": {
            "type": "number"
          },
          "t_exptime": {
            "type": "number"
          },
          "t_resolution": {
            "type": "number"
          },
          "em_xel": {
            "type": "integer",
            "minimum": -2147483648,
            "maximum": 2147483647
          },
          "em_min": {
            "type": "number"
          },
          "em_max": {
            "type": "number"
          },
          "em_res_power": {
            "type": "number"
          },
          "o_ucd": {
            "type": "string",
            "maxLength": 50
          },
          "pol_xel": {
            "type": "integer",
            "minimum": -2147483648,
            "maximum": 2147483647
          },
          "pol_states": {
            "type": "string",
            "maxLength": 11
          },
          "facility_name": {
            "type": "string"
          },
          "instrument_name": {
            "type": "string"
          }
        },
        "required": [
          "dataproduct_type",
          "dataproduct_subtype",
          "calib_level",
          "obs_collection",
          "obs_id",
          "obs_title",
          "access_url",
          "access_format",
          "access_estsize",
          "target_name",
          "s_ra",
          "s_dec",
          "t_min",
          "t_max",
          "t_exptime",
          "facility_name",
          "instrument_name"
        ]
      }
    },
    "required": [
      "interface",
      "execution_block",
      "context",
      "config",
      "files",
      "obscore"
    ]
  }